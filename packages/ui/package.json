    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@turbo/gen": "2.5.0",
    "@types/node": "22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "eslint": "9.29.0",
    "jsdom": "26.1.0",
    "typescript": "5.8.2",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  }
